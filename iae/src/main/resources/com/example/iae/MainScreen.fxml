<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER" minHeight="440.0" minWidth="470.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.iae.MainController">
   <children>
      <VBox alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" spacing="30.0" HBox.hgrow="ALWAYS">
         <children>
            <Label text="Integrated Assignment Environment" textAlignment="CENTER" wrapText="true" />
            <Button mnemonicParsing="false" onAction="#createConfiguration" text="Create New Configuration" />
            <Button mnemonicParsing="false" onAction="#editConfigurations" text="Edit Configurations" />
            <Button mnemonicParsing="false" onAction="#createProject" text="Create New Project" />
            <ListView fx:id="projectList" maxWidth="-Infinity" onContextMenuRequested="#displayProject" onMouseClicked="#displayProject" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets bottom="20.0" top="20.0" />
         </padding></VBox>
      <VBox fx:id="projectScene" alignment="CENTER" prefHeight="600.0" prefWidth="615.0" spacing="30.0" visible="false" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="projectName" text="projectName" />
            <Label fx:id="compilerName" text="compiler/intrepreter name" />
            <Label fx:id="libraryNames" text="library names" />
            <ListView fx:id="projectResults" maxWidth="-Infinity" prefHeight="200.0" prefWidth="200.0" />
            <Button mnemonicParsing="false" onAction="#deleteProject" text="Delete" />
         </children></VBox>
   </children>
   <padding>
      <Insets left="10.0" />
   </padding>
</HBox>
